{
  "roots": [
    "src/",
    "tests/"
  ],
  "builtinLinters": "all",
  "extraLinters": [
    "Facebook\\HHAST\\NoEmptyStatementsLinter",
    "Facebook\\HHAST\\FinalOrAbstractClassLinter",
    "Facebook\\HHAST\\HHClientLinter"
  ],
  "linterConfigs": {
    "Facebook\\HHAST\\HHClientLinter": {
      "__comment": "Ignore duplicate property $this->tagName.",
      "ignore": [
        5635
      ]
    }
  },
  "overrides": [
    {
      "patterns": [
        "tests/*"
      ],
      "disabledLinters": [
        "Facebook\\HHAST\\CamelCasedMethodsUnderscoredFunctionsLinter"
      ]
    },
    {
      "patterns": [
        "src/*"
      ],
      "disabledLinters": [
        "Facebook\\HHAST\\LicenseHeaderLinter"
      ]
    }
  ]
}